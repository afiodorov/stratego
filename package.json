{
  "name": "stratego",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/afiodorv/stratego.git"
  },
  "browser": {
    "jquery": "./public/vendor/jquery.min.js",
    "pnotify": "./public/vendor/pnotify/jquery.pnotify.min.js",
    "jquery-ui": "./public/vendor/jquery-ui.min.js",
    "knockout": "./public/vendor/knockout-2.2.1.js",
    "knockout-jquery": "./public/vendor/knockout-jqueryui.min.js"
  },
  "browserify-shim": {
    "jquery": {"exports": "$"},
    "pnotify": {"exports": "$.pnotify"},
    "jquery-ui": {"exports": null, "depends": ["jquery"]},
    "knockout": {"exports": "ko", "depends": ["jquery"]},
    "knockout-jquery": {"exports": null}
  },
  "browserify": {
    "transform": [ "browserify-shim" ]
  },
  "dependencies": {
    "socket.io": "~0.9.16",
    "express": "3.x",
    "mongodb": "1.1.11",
    "mongoose": "2.6.5",
    "jade": "*",
    "optimist": "0.6.x",
    "session.socket.io": "~0.1.6",
    "connect-mongo": "0.4.x",
    "q": "*",
    "open": "*",
    "browserify-shim": "~3.2.0",
    "grunt-browserify": "2.0.1"
  },
  "engines": {
    "node": "0.10.x",
    "npm": "1.3.x"
  },
  "devDependencies": {
    "grunt": "0.4.2",
    "grunt-watchify": "~0.1.0",
    "grunt-nodemon": "~0.2.0",
    "grunt-concurrent": "~0.4.3",
    "grunt-node-inspector": "~0.1.2",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-contrib-livereload": "~0.1.2",
    "browserify": "~3.26.0"
  }
}
